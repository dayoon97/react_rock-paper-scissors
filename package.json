      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "core-decorators": "^0.20.0",
    "prettier": "2.2.1"
  },
  "barbel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
